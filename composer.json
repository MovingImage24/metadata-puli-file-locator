{
  "name": "mi/metadata-puli-file-locator",
  "description": "Puli-FileLocator for JMS Metadata",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "puli",
    "metadata",
    "jms-serializer"
  ],
  "authors": [
    {
      "name": "Alexander Miehe",
      "email": "alexander.miehe@movingimage.com"
    }
  ],
  "minimum-stability": "beta",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require": {
    "php": ">=5.6.0",
    "jms/metadata": "~1.5.0",
    "puli/repository": "^1.0@beta",
    "puli/discovery": "^1.0@beta"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "symfony/filesystem": "~2.7.0",
    "jms/serializer": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Mi\\Puli\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mi\\Puli\\Tests\\": "tests/"
    }
  }
}
